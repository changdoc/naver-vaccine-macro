{
  "manifest_version": 2,
  "name": "NAVER Vaccine MACRO",
  "description": "네이버 백신 예약 매크로",
  "version": "1.1.2",
  "permissions": [
    "storage",
    "https://v-search.nid.naver.com/",
    "https://m.place.naver.com/rest/vaccine",
    "https://vaccine-map.kakao.com/map"
  ],
  "icons": {
    "96": "icon96.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "css": [],
      "matches": [
        "https://v-search.nid.naver.com/*"
      ],
      "run_at": "document_end"
    },
    {
      "all_frames": true,
      "js": [
        "map.js"
      ],
      "css": [],
      "matches": [
        "https://m.place.naver.com/rest/vaccine*"
      ],
      "run_at": "document_end"
    },
    {
      "all_frames": true,
      "js": [
        "map_kakao.js"
      ],
      "css": [],
      "matches": [
        "https://vaccine-map.kakao.com/map*"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": ["assets/tada.mp3", "assets/boing.mp3"],
  "browser_action": {
    "default_title": "NAVER Vaccine MACRO",
    "default_popup": "popup.html"
  }
}
